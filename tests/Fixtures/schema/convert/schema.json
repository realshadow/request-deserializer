{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 3
        },
        "mass": {
            "type": "float",
            "minimum": 1,
            "maximum": 100
        },
        "habitable": {
            "type": "boolean"
        }
    },
    "required": [
        "name",
        "mass"
    ]
}
